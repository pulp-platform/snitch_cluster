{
    setup_ssr: 1,
    parallelize_m: 0,
    parallelize_k: 0,
    m_tiles: 2, // number of tiles in M dimension
    n_tiles: 2, // number of tiles in N dimension
    k_tiles: 2, // number of tiles in K dimension
    load_a: 1,
    load_b: 1,
    load_c: 1,
    transa: false,
    transb: false, // must be true for SIMD
    M: 16,
    N: 16,
    K: 8,
    alpha: 1,
    beta: 0,
    gemm_fp: "gemm_fp64_opt"
}
